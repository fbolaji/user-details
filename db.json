{
  "formConfigData": {
    "userForm": [
      {
        "label": "Name *",
        "id": "name",
        "name": "name",
        "type": "text",
        "placeholder": "Please enter your name",
        "isRequired": true,
        "validator": {
          "isRequired": true,
          "error": "Name is required."
        }
      },
      {
